{
  "name": "cpr_testdir",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass ./src/sass/main.scss ./bin_dev/css/style.min.css -w",
    "compile:sass": "node-sass src/sass/main.scss ./bin_dev/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" ./bin_dev/css/style.comp.css -o ./bin_dev/css/style.prefix.css",
    "compress:css": "uglifycss ./bin_dev/css/style.prefix.css > ./bin_dev/css/style.min.css",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "begin": "webpack-dev-server --mode development --open",
    "createcpr": "npm install cpr_pathpackage",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "npm-run-all --parallel watch:sass begin",
    "server": "nodemon server.js",
    "server:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "fbuild": "npm-run-all createcpr build:css build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^8.6.5",
