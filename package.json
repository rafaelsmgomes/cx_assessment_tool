    "concat": "^1.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.1",
    "uglifycss": "0.0.29",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bodymovin": "^4.13.0",
    "cors": "^2.8.5",
    "cpr-rslider": "^1.0.0",
    "cpr_pathpackage": "^1.1.70",
    "css-loader": "^3.2.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
